{% extends 'base.html.twig' %}

{% block title %}ProduktDoUtylizacji{% endblock %}

{% block body %}


    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ produkt_do_utylizacji.id }}</td>
            </tr>
            <tr>
                <th>Nazwa</th>
                <td>{{ produkt_do_utylizacji.nazwa }}</td>
            </tr>
            <tr>
                <th>NumerSeryjny</th>
                <td>{{ produkt_do_utylizacji.numerSeryjny }}</td>
            </tr>
            <tr>
                <th>DataProdukcji</th>
                <td>{{ produkt_do_utylizacji.dataProdukcji ? produkt_do_utylizacji.dataProdukcji|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>PlanowanaDataUtylizacji</th>
                <td>{{ produkt_do_utylizacji.PlanowanaDataUtylizacji ? produkt_do_utylizacji.PlanowanaDataUtylizacji|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CzyZostaloZutylizowane</th>
                <td>{{ produkt_do_utylizacji.CzyZostaloZutylizowane ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_produkt_do_utylizacji_index') }}">Wróć do listy</a>
    {% if (is_granted('ROLE_ADMIN'))%}
    <a href="{{ path('app_produkt_do_utylizacji_edit', {'id': produkt_do_utylizacji.id}) }}">Edytuj</a>
        {%endif%}
    {{ include('produkt_do_utylizacji/_delete_form.html.twig') }}
{% endblock %}
